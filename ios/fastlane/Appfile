# Appfile - iOS App Configuration

# Your Apple ID email
# Replace with your actual Apple ID or set via environment variable
app_identifier("com.example.app") # The bundle identifier of your app
apple_id(ENV["APPLE_ID"]) # Your Apple ID email

# App Store Connect Team ID
itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"]) # Optional, if you have multiple teams

# Developer Portal Team ID
team_id(ENV["FASTLANE_TEAM_ID"]) # Optional, if you have multiple teams

